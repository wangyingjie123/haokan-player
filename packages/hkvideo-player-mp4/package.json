{
    "name": "hkvideo-player-mp4",
    "version": "1.0.0-3",
    "description": "hkvideo-player plugin for mp4 transform to fmp4",
    "main": "./dist/index.js",
    "scripts": {
        "prepare": "npm run build",
        "build": "webpack --progress --display-chunks -p",
        "dev": "webpack --progress --display-chunks -p --watch",
        "test": "karma start --single-run",
        "test:watch": "karma start"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wangyingjie123/hkvideo-player/tree/master/packages/hkvideo-player-mp4"
    },
    "babel": {
        "presets": [
            "es2015"
        ],
        "plugins": [
            "add-module-exports",
            "babel-plugin-bulk-import"
        ]
    },
    "keywords": [
        "mp4",
        "fmp4",
        "player",
        "video"
    ],
    "author": "wangyingjie07",
    "license": "MIT",
    "dependencies": {
        "concat-typed-array": "^1.0.2",
        "deepmerge": "^2.0.1",
        "event-emitter": "^0.3.5",
        "hkvideo-player": "^1.0.1-3"
    },
    "peerDependency": {
        "hkvideo-player": "^1.0.1-3"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-bulk-import": "^1.0.2",
        "babel-preset-es2015": "^6.24.1",
        "css-loader": "^3.6.0",
        "json-loader": "^0.5.7",
        "node-sass": "^4.14.1",
        "postcss-cssnext": "^3.1.0",
        "postcss-loader": "^2.1.5",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    }
}
