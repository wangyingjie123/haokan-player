{
    "name": "hkvideo-player",
    "version": "1.0.3-1",
    "description": "video player",
    "main": "./dist/index.js",
    "typings": "./types/index.d.ts",
    "scripts": {
        "build": "NODE_ENV=production webpack --progress",
        "dev": "NODE_ENV=development webpack --progress --watch",
        "start": "webpack-dev-server --config ./webpack.config.js --mode development  --progress --display-chunks -p --watch"
    },
    "keywords": [
        "video",
        "player"
    ],
    "sideEffects": [
        "@babel/polly-fill",
        "*.scss"
    ],
    "babel": {
        "presets": [
            "es2015"
        ],
        "plugins": [
            "add-module-exports",
            "babel-plugin-bulk-import"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wangyingjie123/hkvideo-player"
    },
    "author": "2969057196@qq.com",
    "license": "MIT",
    "browserslist": [
        "> 5%",
        "IE 9",
        "iOS 7",
        "Firefox > 20"
    ],
    "dependencies": {
        "danmu.js": "^0.2.23",
        "deepmerge": "^4.2.2",
        "downloadjs": "^1.4.7",
        "draggabilly": "^2.3.0",
        "event-emitter": "^0.3.5",
        "pasition": "^1.0.1",
        "request-frame": "^1.5.3",
        "ua-parser-js": "^0.7.22"
    },
    "devDependencies": {
        "autoprefixer": "^9.1.5",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-bulk-import": "^1.0.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "chai": "^4.1.2",
        "core-js": "^2.5.4",
        "css-loader": "^5.0.0",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "postcss-cssnext": "^3.1.0",
        "postcss-loader": "^4.0.4",
        "raw-loader": "^4.0.1",
        "sass-loader": "^10.0.4",
        "style-loader": "^2.0.0",
        "svgo": "^1.3.2",
        "svgo-loader": "^2.2.1",
        "thread-loader": "^3.0.0",
        "webpack": "^5.2.0",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^4.1.0",
        "webpack-dev-server": "^3.11.0"
    }
}
